<div class="container text-light">
    <h1 class="text-center mt-2">Newest Articles</h1>
    <ul class="list-group mt-5 mb-5 text-light">
        {{#each posts as |post|}}
        <li class="list-group-item bg-dark text-light">
            {{> post-info post}}
        </li>
        {{/each}}
    </ul>
</div>